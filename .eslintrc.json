{
  "env": {
    "browser": true
  },
  "extends": [
    "@neilbryson",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": "off",
    "semi": "off",
    "@typescript-eslint/indent": ["warn", 2],
    "@typescript-eslint/semi": ["warn"]
  }
}
